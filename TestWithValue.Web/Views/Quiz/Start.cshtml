@using TestWithValue.Domain.ViewModels.Test
@model IEnumerable<ShowTestViewModel>
@{
    ViewData["Title"] = "Start";
}


@await Component.InvokeAsync("ActionMessage")


<h2>لیست آزمون‌ها</h2>
<ul>
    @foreach (var test in Model)
    {
        <li>
            <a href="@Url.Action("ShowQuestions", "Quiz", new { testId=test.TestId })">@test.TestName</a>
        </li>
    }
</ul>

